{
  "name": "utopia",
  "version": "2.0.0",
  "description": "CSS compiler for the Utopia system",
  "scripts": {
    "build": "sass src/scss/utopia.scss css/utopia.css --style=expanded --no-source-map",
    "watch": "sass src/scss/utopia.scss css/utopia.css --style=expanded --source-map --watch",
    "unpack": "node ./utils/foundry-unpack.mjs",
    "pack": "node ./utils/foundry-pack.mjs"
  },
  "browserslist": [
    "last 3 versions"
  ],
  "author": "Asacolips",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@foundryvtt/foundryvtt-cli": "^1.0.3",
    "sass": "^1.53.0"
  },
  "dependencies": {
    "codemirror": "^6.0.1",
    "monaco-editor": "^0.52.2",
    "select2": "^4.1.0-rc.0"
  }
}
