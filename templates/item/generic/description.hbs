<div class="tab description {{tab.cssClass}}"  data-group="primary" data-tab="description">
  <fieldset>
    <legend>{{localize "UTOPIA.CommonTerms.description"}}</legend>

    {{#if editable}}
      <prose-mirror name="system.description" data-document-u-u-i-d="{{item.uuid}}" value="{{system.description}}" collaborate="true" toggled="true">
        {{{enrichedDescription}}}
